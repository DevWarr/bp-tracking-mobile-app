{
  "extends": "tslint:recommended",
  "linterOptions": {
    "exclude": ["./**/node_modules"]
  },
  "rules": {
    "max-classes-per-file": false,
    "no-unused-variable": true,
    "ordered-imports": [
      true,
      {
        "import-sources-order": "lowercase-last",
        "named-imports-order": "lowercase-first"
      }
    ],
    "eofline": true
  }
}
